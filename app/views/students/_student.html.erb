<div id="<%= dom_id student %>">
  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= student.email %>
  </p>

  <p>
    <strong>Department:</strong>
    <%= student.department %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= student.phone %>
  </p>

  <p>
    <strong>Roll no:</strong>
    <%= student.roll_no %>
  </p>

  <p>
    <strong>Vehicle no:</strong>
    <%= student.vehicle_no %>
  </p>

  <p>
    <strong>Adhar no:</strong>
    <%= student.adhar_no %>
  </p>

  <p>
    <strong>License:</strong>
    <%= student.license %>
  </p>

    <% if student.aadhar_img.attached? %>
      <%= image_tag student.aadhar_img %>
    <%end%>

    <% if student.license_img.attached? %>
      <%= image_tag student.license_img %>
    <%end%>

    <% if student.photo.attached? %>
      <%= image_tag student.photo %>
    <%end%>

    <% if student.registration_img.attached? %>
      <%= image_tag student.registration_img %>
    <%end%>


    
          

</div>
